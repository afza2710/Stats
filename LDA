library(MASS)
library(tidyverse)
library(caret)
library(ggplot2)
library(car)
library(rattle)
data=read.csv('C://Users//Afza//OneDrive//Documents//data.csv')
colSums(is.na(data))
data=data[,2:32]
data$diagnosis=as.factor(data$diagnosis)
str(data)
b=boxplot(data)
model=lda(diagnosis~.,data=data)
summary(model)
model.values=predict(model)
head(model.values,5)
names(model.values)
dim(model.values$x)
dim(model.values$class)
dim(model.values$posterior)
ldahist(data=model.values$x[,1],g=data$diagnosis) 
plot(model.values$x[,1],model.values$posterior[,2]) 
text(model.values$x[,1],model.values$posterior[,2],data$diagnosis,cex=0.7,pos=4,col='red') 
C=confusionMatrix(model.values$class,data$diagnosis)
C
